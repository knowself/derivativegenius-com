{
  "version": 2,
  "framework": "vue",
  "buildCommand": "bash build.sh",
  "outputDirectory": ".vercel/output",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.8",
    "NODE_VERSION": "20.x"
  },
  "functions": {
    "api/wsgi.py": {
      "runtime": "vercel-python@3.1.0",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.8",
      "NODE_VERSION": "20.x"
    }
  }
}
